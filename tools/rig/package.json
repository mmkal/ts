{
  "name": "@mmkal/rig",
  "version": "0.0.1",
  "private": true,
  "description": "All-in-one dev dependency - designed for packages within this repo, but in theory could be used externally too",
  "bin": {
    "rig": "./rig.js",
    "init": "./init.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "node rig rimraf dist",
    "prebuild": "npm run clean",
    "build": "node rig tsc -p .",
    "lint": "node rig eslint --cache .",
    "test": "node rig jest"
  },
  "dependencies": {
    "typescript": "^4.0.2",
    "@types/jest": "~26.0.13",
    "@types/node": "^14.10.1",
    "eslint": "~7.9.0",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-unicorn": "~21.0.0",
    "eslint-plugin-jest": "~24.0.1",
    "eslint-plugin-codegen": "~0.12.0",
    "prettier": "~2.1.1",
    "eslint-config-xo": "~0.32.1",
    "eslint-config-xo-typescript": "~0.32.0",
    "eslint-plugin-import": "~2.22.0",
    "@typescript-eslint/eslint-plugin": "~4.1.0",
    "@typescript-eslint/parser": "~4.1.1",
    "jest": "~26.4.2",
    "ts-jest": "~26.3.0",
    "eson-parser": "~0.0.5",
    "find-up": "~5.0.0",
    "@actions/github": "~4.0.0",
    "lodash": "^4.17.15",
    "js-yaml": "^3.14.0",
    "@types/js-yaml": "~3.12.5",
    "rimraf": "~3.0.2",
    "concurrently": "~5.3.0",
    "webpack": "~4.44.2",
    "webpack-cli": "~3.3.12",
    "ts-loader": "~8.0.4"
  },
  "devDependencies": {
    "@types/eslint": "~7.2.2",
    "@microsoft/rush-lib": "5.34.0-pr2172.0",
    "@types/lodash": "~4.14.161"
  }
}
