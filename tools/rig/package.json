{
  "name": "@mmkal/rig",
  "version": "0.0.1",
  "private": true,
  "description": "All-in-one dev dependency - designed for packages within this repo, but in theory could be used externally too",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmkal/ts.git",
    "directory": "tools/rig"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "init": "./init.js",
    "rig": "./rig.js"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "node rig tsc -p .",
    "clean": "node rig rimraf dist",
    "lint": "node rig eslint --cache .",
    "test": "node rig jest"
  },
  "dependencies": {
    "@actions/github": "4.0.0",
    "@types/jest": "26.0.19",
    "@types/js-yaml": "3.12.5",
    "@types/node": "14.14.14",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "check-clean": "0.3.0",
    "concurrently": "5.3.0",
    "eslint": "7.15.0",
    "eslint-config-xo": "0.33.1",
    "eslint-config-xo-typescript": "0.37.0",
    "eslint-plugin-codegen": "0.14.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-unicorn": "21.0.0",
    "eson-parser": "0.0.5",
    "find-up": "5.0.0",
    "jest": "26.6.3",
    "js-yaml": "3.14.1",
    "lodash": "4.17.20",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sort-package-json": "1.48.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.12",
    "typescript": "4.1.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  },
  "devDependencies": {
    "@microsoft/rush-lib": "5.35.2",
    "@types/eslint": "7.2.6",
    "@types/lodash": "4.14.165"
  }
}
