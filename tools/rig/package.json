{
  "name": "@mmkal/rig",
  "version": "0.0.1",
  "private": true,
  "description": "All-in-one dev dependency - designed for packages within this repo, but in theory could be used externally too",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmkal/ts.git",
    "directory": "tools/rig"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "init": "./init.js",
    "rig": "./rig.js"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "node rig tsc -p .",
    "clean": "node rig rimraf dist",
    "lint": "node rig eslint --cache .",
    "test": "node rig jest"
  },
  "dependencies": {
    "@actions/github": "4.0.0",
    "@types/jest": "26.0.24",
    "@types/js-yaml": "3.12.7",
    "@types/node": "14.18.32",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "check-clean": "0.3.1",
    "concurrently": "5.3.0",
    "eslint": "7.32.0",
    "eslint-config-xo": "0.42.0",
    "eslint-config-xo-typescript": "0.53.0",
    "eslint-plugin-codegen": "0.16.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-unicorn": "21.0.0",
    "eson-parser": "0.0.6",
    "find-up": "5.0.0",
    "jest": "26.6.3",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.15",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sort-package-json": "1.57.0",
    "ts-jest": "26.5.6",
    "ts-loader": "8.4.0",
    "typescript": "4.8.4",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12"
  },
  "devDependencies": {
    "@microsoft/rush-lib": "5.82.1",
    "@types/eslint": "7.29.0",
    "@types/lodash": "4.14.186"
  }
}
