{
  "packages": ["packages/*"],
  "version": "independent",
  "toposort": true,
  "stream": true,
  "command": {
    "publish": {
      "conventionalCommits": true,
      "allowBranch": ["main"],
      "exact": true,
      "createRelease": "github"
    }
  },
  "ignoreChanges": ["**/__tests__/**"]
}
